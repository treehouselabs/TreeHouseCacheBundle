{
  "name": "treehouselabs/cache-bundle",
  "type": "symfony-bundle",
  "description": "Symfony2 bundle that provides easy setup for caching, including a powerful ORM cache",
  "keywords": ["cache"],
  "license": "MIT",
  "authors": [
    {
      "name": "Peter Kruithof",
      "email": "peter@treehouse.nl"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.5",
    "symfony/symfony": "^2.6|^3.0",
    "doctrine/orm": "^2.2.3",
    "doctrine/doctrine-bundle": "^1.2",
    "treehouselabs/cache": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.1"
  },
  "autoload": {
    "psr-4": {
      "TreeHouse\\CacheBundle\\": "src/TreeHouse/CacheBundle/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TreeHouse\\CacheBundle\\Tests\\": "tests/src/TreeHouse/CacheBundle/Tests/",
      "TreeHouse\\FunctionalTestBundle\\": "tests/src/TreeHouse/FunctionalTestBundle/"
    }
  }
}
